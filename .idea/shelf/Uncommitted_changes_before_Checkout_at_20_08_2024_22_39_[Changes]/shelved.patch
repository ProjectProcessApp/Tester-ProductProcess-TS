+        anamenu.AnasayfaHeader.click();
+        Thread.sleep(1000);
+        Assert.assertEquals("Gasan Üretim Yönetim Sistemi",Driver.getDriver().getTitle());
+
+        anamenu.herhangibiryer.click();
+        Thread.sleep(1000);
+        Assert.assertEquals("Gasan Üretim Yönetim Sistemi",Driver.getDriver().getTitle());
+    }
+
+    @Then("Talasli Imalat Amiri Butonu gorunur oldugu dogrulanir")
